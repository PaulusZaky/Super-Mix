{
  "wrapper": "div.collection-wrapper",
  "sections": {
    "search_header": {
      "type": "template--search-header",
      "settings": {}
    },
    "main": {
      "type": "template--search",
      "blocks": {
        "grid-card-title": {
          "type": "grid-card-title",
          "settings": {}
        },
        "grid-card-price": {
          "type": "grid-card-price",
          "disabled": true,
          "settings": {}
        },
        "grid-card-swatches": {
          "type": "grid-card-swatches",
          "settings": {}
        }
      },
      "block_order": [
